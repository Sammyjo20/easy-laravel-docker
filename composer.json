{
    "name": "sammyjo20/easy-laravel-docker",
    "type": "project",
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.26",
        "illuminate/console": "^12.26",
        "laravel/prompts": "^0.3.6"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^4.0.4",
        "spatie/ray": "^1.42"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sammyjo20\\EasyLaravelDocker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sammyjo20\\EasyLaravelDocker\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sammyjo20",
            "email": "29132017+Sammyjo20@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sammyjo20\\EasyLaravelDocker\\EasyLaravelDockerServiceProvider"
            ]
        }
    }
}
